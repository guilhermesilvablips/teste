        "id": "8d15b591-b005-46ce-8f20-427039767d6f",
        "name": "Sheet Id"
      },
      {
        "parameters": {
          "jsCode": "// 1) inputs\nconst anteriores = $(\"Dados Anteriores\").first().json;  // { NOME: \"...\", DOCUMENTO: \"...\", ‚Ä¶ }\nconst novos      = $(\"ARGS\").first().json;             // { INDEX_LINHA: 2, LOG_ENVIO: null, EMAIL_ENVIADO: null, ‚Ä¶ }\n\n// 2) pega o n√∫mero da linha\nconst row = novos.INDEX_LINHA;\n\n// 3) monta dinamicamente a lista de cabe√ßalhos (na ordem de inser√ß√£o)\nconst headers = Object.keys(anteriores);\n\n// 4) converte √≠ndice (0,1,2‚Ä¶) em letra de coluna (A, B, C ‚Ä¶ Z, AA, AB‚Ä¶)\nfunction colLetter(idx) {\n  let letter = '';\n  while (idx >= 0) {\n    letter = String.fromCharCode(65 + (idx % 26)) + letter;\n    idx = Math.floor(idx / 26) - 1;\n  }\n  return letter;\n}\n\n// 5) percorre cada campo que veio em `novos`, compara com o anterior\nconst sheetName = 'Base Blips';\nconst updates = [];\n\nfor (const key of Object.keys(novos)) {\n  if (key === 'INDEX_LINHA') continue;        // pula o √≠ndice da linha\n\n  const newVal = novos[key];\n  if (newVal == null) continue;              // pula se veio null ou undefined\n\n  const oldVal = anteriores[key];\n  if (oldVal !== newVal) {\n    const colIdx = headers.indexOf(key);\n    if (colIdx === -1) continue;             // coluna n√£o mapeada\n\n    const letter = colLetter(colIdx);\n    updates.push({\n      range: `${sheetName}!${letter}${row}`,\n      values: [[ newVal ]]\n    });\n  }\n}\n\n// 6) retorna o array pronto para o pr√≥ximo n√≥\nreturn [{\n  json: {\n    data: updates\n  }\n}];"
        },
